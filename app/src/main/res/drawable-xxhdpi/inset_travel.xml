<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <inset
            android:insetBottom="@dimen/dp_6"
            android:insetLeft="@dimen/dp_6"
            android:insetRight="@dimen/dp_6"
            android:insetTop="@dimen/dp_6">
            <shape android:shape="oval">
                <size
                    android:width="@dimen/dp_20"
                    android:height="@dimen/dp_20" />
                <solid android:color="#e4dfe1" />
            </shape>
        </inset>
    </item>
    <item android:gravity="bottom|end"
        android:end="@dimen/dp_1"
        android:bottom="@dimen/dp_1">
        <bitmap
            android:antialias="true"
            android:gravity="center"
            android:src="@drawable/icon_right"></bitmap>
    </item>
</layer-list>